<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AWLike.LoginPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="Username or Email" Text="{Binding LoginUser.Username}"/>
            <Entry Placeholder="Password" IsPassword="True" Text="{Binding LoginUser.Password}"/>
            <Button Text="login" Command="{Binding LoginCommand}"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>